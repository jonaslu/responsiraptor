    display: table;
    padding-top: 20px;
    margin: 0px auto;
    position: relative;
    }
    .beforescissors {
    vertical-align: middle;
    display: table-cell;
    width: 45%;
    border-bottom: 1px dashed;
    top: -15px;
    position: relative;
    left: -20px;
    }
    .afterscissors {
    vertical-align: middle;
    display: table-cell;
    width: 45%;
    border-bottom: 1px dashed;
    top: -15px;
    position: relative;
    right: -20px;
    }
    .container{
    max-width: 900px;
    }
    body { 
    font-weight: 200;
    font-size: 24px;
    line-height: 1.4;
    font-family: Georgia, &quot;Times New Roman&quot;, Times, serif;
    color: #494e55;
    background-color: #eeeee6;
    }
    a {
    -moz-transition:color .2s ease-in;
    -o-transition:color .2s ease-in;
    -webkit-transition:color .2s ease-in;
    color: #F01D3F;
    text-decoration: none
    }
    a:hover,a:focus {
    color: #FF0000;
    text-decoration: underline
    }
    a:focus {
    outline: 0px;
    }
    h1 {
    font-weight: bold;
    line-height: 1.1;
    font-family: &quot;Raleway&quot;, &quot;Helvetica Neue&quot;, Helvetica, Arial, sans-serif;
    }
    .logo {
			font-size: 60px;
			font-family: &quot;Fredericka the great&quot;, cursive;
	}
    @font-face {
    font-family: &#39;Raleway&#39;;
    font-style: normal;
    font-weight: 400;
    src: local(&#39;Raleway&#39;), url(http://themes.googleusercontent.com/static/fonts/raleway/v7/IczWvq5y_Cwwv_rBjOtT0w.woff) format(&#39;woff&#39;);
    }
    @font-face {
    font-family: &#39;Raleway&#39;;
    font-style: normal;
    font-weight: 700;
    src: local(&#39;Raleway Bold&#39;), local(&#39;Raleway-Bold&#39;), url(http://themes.googleusercontent.com/static/fonts/raleway/v7/JbtMzqLaYbbbCL9X6EvaIxsxEYwM7FgeyaSgU71cLG0.woff) format(&#39;woff&#39;);
    }

    .header {
    border-bottom: 1px solid #999;
    }
    .main {
    padding-top: 20px;
    margin-bottom: 20px;
    }
    .footer {
    font-family: &quot;Helvetica Neue&quot;,Helvetica,Arial,sans-serif;
    font-size: 14px;
    line-height: 1.42857143;
    border-top: 1px solid #999;
    text-align: center;
    height: 70px;
    padding-top: 25px;
    }
    .logo &gt; a {
    -moz-transition:none;
    -o-transition:none;
    -webkit-transition:none;
    color: #494e55;
    text-decoration: none
    }
    .logo &gt; a:hover,a:focus {
    color: #494e55;
    text-decoration: none
    }
    .nav-pills &gt; li {
    font-family: &quot;Helvetica Neue&quot;,Helvetica,Arial,sans-serif;
    font-size: 14px;
    line-height: 1.42857143;
    }
    .nav-pills &gt; li.active &gt; a, .nav-pills&gt;li.active&gt;a:hover, .nav-pills&gt;li.active&gt;a:focus {
    border-radius: 0px;
    background-color: #eeeee6;
    color: #FD2B2B;
    }
    .nav-pills &gt; li &gt; a, .nav-pills&gt;li&gt;a:hover, .nav-pills&gt;li&gt;a:focus {
    border-radius: 0px;
    background-color: #eeeee6;
    color: #494e55;	
    }
    .nav-pills &gt; li.active &gt; a, .nav-pills&gt;li.active&gt;a:hover, .nav-pills&gt;li.active&gt;a:focus {
    border-bottom: 2px solid #FD2B2B;
    }
    .nav-pills&gt;li&gt;a:hover, .nav-pills&gt;li&gt;a:focus {
    border-bottom: 2px solid #999;
    }
    @media (min-width: 768px) {
    .nav { 
    float: right; 
    margin-top: 37px;
    }
    }
    @media (max-width: 768px) {
    .nav, .logo { 
    margin-top: 0px;
    float: none;
    display: table !important;
    margin-left: auto; 
    margin-right: auto 
    }
    .nav {
    margin-bottom: 15px;
    }
    }
    #blog-pager {
    display:none;
    }
    .blog-feeds {
    display:none;
    }

	/*	 -- Prettify -- */
	/*	Display line nums on all lines */
	li.L0, li.L1, li.L2, li.L3, li.L5, li.L6, li.L7, li.L8 {
		list-style-type: decimal;
	}
	/* Make pre code box span entire width */
	pre.prettyprint {
		width: auto;

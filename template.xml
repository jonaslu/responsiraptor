  <body>
    <div class='content'>
      <div class='container'>
        <div class='row header'>
          <div class='col-sm-6'>
            <span class='logo'>
              <a expr:href='data:blog.homepageUrl'><data:blog.title/></a>
            </span>
          </div>
          <div class='col-sm-6'>
            <ul class='nav nav-pills'>
              &lt;li class=&quot;<b:if cond='data:blog.url == data:blog.homepageUrl'>active</b:if>&quot;&gt;
				<a expr:href='data:blog.homepageUrl'>
                  How to
                </a>
              &lt;/li&gt;
              &lt;li class=&quot;<b:if cond='data:blog.pageName == &quot;Github&quot;'>active</b:if>&quot;&gt;
                <a href='/p/github.html'>
                  Get it
                </a>
              &lt;/li&gt;
    		  &lt;li class=&quot;<b:if cond='data:blog.pageName == &quot;About&quot;'>active</b:if>&quot;&gt;
                <a href='/p/about.html'>
                  About
                </a>
              &lt;/li&gt;
            </ul>
          </div>
        </div>
        <div class='row main'>
          <div class='col-sm-12'>
            <b:section id='post'>
              <b:widget id='Blog1' locked='true' title='Blog Posts' type='Blog'>
                <b:includable id='main' var='post'>
					<b:if cond='data:blog.pageType == &quot;item&quot;'>
                        <b:include data='post' name='post'/>
					</b:if>
                    <b:if cond='data:blog.pageType == &quot;static_page&quot;'>
                        <b:include data='post' name='post'/>
                    </b:if>
                    <b:if cond='data:blog.pageType == &quot;index&quot;'>
					  <b:include data='post' name='postlist'/>
                    </b:if>
                </b:includable>
                <b:includable id='backlinkDeleteIcon'/>
                <b:includable id='backlinks'/>
                <b:includable id='comment-form'/>
                <b:includable id='commentDeleteIcon'/>
                <b:includable id='comment_count_picker' var='post'/>
                <b:includable id='comment_picker' var='post'/>
                <b:includable id='comments' var='post'/>
                <b:includable id='feedLinks'/>
                <b:includable id='feedLinksBody'/>
                <b:includable id='iframe_comments' var='post'/>
                <b:includable id='labels' var='post'/>
                <b:includable id='mobile-index-post'/>
                <b:includable id='mobile-main' var='top'/>
                <b:includable id='mobile-nextprev'/>
                <b:includable id='mobile-post' var='post'/>
                <b:includable id='nextprev'/>
                <b:includable id='post' var='post'>
                    <b:loop values='data:posts' var='post'>
                        <h1><data:post.title/></h1>
                        <data:post.body/>
                        <div class='disqus_thread' id='disqus_thread'/>
                        <script type='text/javascript'>
			    <!-- Disqus settings -->
                            /* * * CONFIGURATION VARIABLES: EDIT BEFORE PASTING INTO YOUR WEBPAGE * * */
                            var disqus_shortname = &#39;$replace_with_disqus_short_name$#39;; // Required - Replace example with your forum shortname
                            var disqus_identifier = window.disqus_identifier || &#39;<data:post.url/>&#39;;

                            /* * * DON&#39;T EDIT BELOW THIS LINE * * */
                            (function() {
                                var dsq = document.createElement(&#39;script&#39;); dsq.type = &#39;text/javascript&#39;; dsq.async = true;
                                dsq.src = &#39;//&#39; + disqus_shortname + &#39;.disqus.com/embed.js&#39;;
                                (document.getElementsByTagName(&#39;head&#39;)[0] || document.getElementsByTagName(&#39;body&#39;)[0]).appendChild(dsq);
                            })();
                        </script>
                        <noscript>Please enable JavaScript to view the <a href='http://disqus.com/?ref_noscript'>comments powered by Disqus.</a></noscript>
                        <a class='dsq-brlink' href='http://disqus.com'>blog comments powered by <span class='logo-disqus'>Disqus</span></a>
                  </b:loop>                    
                </b:includable>
                <b:includable id='postQuickEdit'/>
                <b:includable id='postlist' var='post'>
                  <b:loop values='data:posts' var='post'>
                    <h1><a expr:href='data:post.url'><data:post.title/></a></h1>
                    <b:loop values='data:post.labels' var='label'>
                      <p><data:label.name/></p>
                    </b:loop>
                    <span class='timestamp'><data:post.timestamp/></span>
                    <div class='scissorstable'>
                      <span class='beforescissors'/>
                      <span class='fa fa-cut'/>
                      <span class='afterscissors'/>
                    </div>
                  </b:loop>
				  <div class='row'>
					<div class='col-xs-6'>
                      <b:if cond='data:olderPageTitle'>
                        <span class='fa fa-arrow-circle-o-left'/><a expr:href='data:olderPageUrl'> Older entries</a>
                      </b:if>
        		    </div>
					<div class='col-xs-6'>
						<div class='pull-right'>
                        <b:if cond='data:newerPageTitle'>
                          <a expr:href='data:newerPageUrl'>Newer entries </a><span class='fa fa-arrow-circle-o-right'/>
                        </b:if>
						</div>
                    </div>
                  </div>
                </b:includable>
                <b:includable id='shareButtons'/>
                <b:includable id='status-message'/>
                <b:includable id='threaded-comment-form' var='post'/>
                <b:includable id='threaded_comment_js' var='post'/>
                <b:includable id='threaded_comments' var='post'/>
              </b:widget>
            </b:section>
          </div>
        </div>
        <div class='row footer'>
          Footer stuff. Responsiraptor theme by <a href='http://catchmecode.com'>Jonas</a>.
        </div>
      </div>
    </div>
